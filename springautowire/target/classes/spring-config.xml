<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean name="add1" class="springautowire.model.Address">
<property name="city" value="Vadodara"/>
<property name="state" value="Gujarat"/>
</bean>

<bean name="employee" class="springautowire.model.Employee" autowire="byType">
<property name="empName" value="Narendra Modi"/>
</bean>


</beans>